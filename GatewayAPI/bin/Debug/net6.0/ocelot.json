{
  //upstreamPathTemplate = her vi reciver vores request
  //downstreamPathTemplete = hvor vi sender den videre til
  "Routes": [
    {
      "UpstreamPathTemplate": "/api/WeatherForecast",
      "UpstreamHttpMethod": [ "Get", "Post", "Put" ],
